{
  "expo": {
    "name": "LabelingApp",
    "slug": "LabelingApp",
    "owner": "logivations_dev",
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "splash": {
      "image": "./assets/loading-page.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "extra": {
      "isDevMode": false
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "Logivations GmbH",
            "project": "LabelingApp",
            "authToken": "0c41969ef75d436bb51b716b940d7ec2fa08b7406d774289bc98c13b87b297b2"
          }
        }
      ]
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.logivations.labelingapp",
      "buildNumber": "1.3.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.logivations.labelingapp",
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "sentry-expo"
    ]
  }
}
